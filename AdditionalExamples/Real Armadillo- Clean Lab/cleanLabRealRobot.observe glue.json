{
    "PlpMain": {
        "Project": "cleanLabRealRobot",
        "Name": "observe",
        "Type": "Glue",
        "Version": 1.2
    },
    "GlueFramework": "ROS",
    "ModuleResponse": {
        "ResponseRules": [
            {
                "Response": "eNotObserveCan",
                "ConditionCodeWithLocalVariables": "goal_reached == True"
            },
            {
                "Response": "eObserveCan",
                "ConditionCodeWithLocalVariables": "goal_reached == False"
            }
        ]
    },
    "ModuleActivation": {
        "RosService": {
            "ImportCode": [
                {
                    "From": "geometry_msgs.msg",
                    "Import": [
                        "Point"
                    ]
                },
                {
                    "From": "simple_navigation_goals.srv",
                    "Import": [
                        "navigate",
                        "navigateResponse"
                    ]
                }
            ],
            "ServicePath": "/navigate_to_point",
            "ServiceName": "navigate",
            "ServiceParameters": [
                {
                    "ServiceFieldName": "goal",
                    "AssignServiceFieldCode": "Point(x= nav_to_x, y= nav_to_y, z= nav_to_z)"
                }
            ]
        }
    },
    "LocalVariablesInitialization": [
        {
            "LocalVariableName": "goal_reached2",
            "RosTopicPath": "/rosout",
            "VariableType": "bool",
            "InitialValue": "False",
            "TopicMessageType": "Log",
            "ImportCode": [
                {
                    "From": "rosgraph_msgs.msg",
                    "Import": [
                        "Log"
                    ]
                }
            ],
            "AssignmentCode": "if goal_reached == True:\n\treturn True\nelse:\n\treturn __input.msg.find('Goal reached') > -1"
        }
    ]
}